{
  "name": "redux-state-actions",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/redux-state/actions/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/redux-state/actions"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/libs/redux-state/actions"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/redux-state/actions"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/redux-state/actions/**/*.ts"]
      }
    }
  },
  "tags": []
}
